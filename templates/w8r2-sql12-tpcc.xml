    	<transactiontype>
    		<name>Payment</name>
    		<id>2</id>
    	</transactiontype>
    	<transactiontype>
    		<name>OrderStatus</name>
    		<id>3</id>
    	</transactiontype>
    	<transactiontype>
    		<name>Delivery</name>
    		<id>4</id>
    	</transactiontype>
    	<transactiontype>
    		<name>StockLevel</name>
    		<id>5</id>
    	</transactiontype>
   	</transactiontypes>
</parameters>
